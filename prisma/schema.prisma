// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id        Int      @default(autoincrement()) @id
  name      String
  price1    Decimal
  price2    Decimal
  price3    Decimal
  price4    Decimal
}
